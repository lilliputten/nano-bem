    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "babel-plugin-directory-resolver": "^1.1.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-preset-es2015": "^6.24.1",
    "bem-xjst": "^8.9.5",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^3.0.0",
    "dateformat": "^3.0.3",
