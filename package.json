    "postcss-nested-ancestors": "^2.0.0",
    "postcss-reporter": "^6.0.1",
    "postcss-simple-vars": "^5.0.2",
    "postcss-url": "^8.0.0",
    "style-loader": "^0.23.1",
    "stylelint": "^10.1.0",
    "stylelint-scss": "^3.8.0",
    "uglify-js": "^3.6.0",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "webpack": "^4.35.0",
    "webpack-build-notifier": "^1.0.3",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.7.2",
    "webpack-file-preprocessor-plugin": "0.0.1"
  },
  "scripts": {
    "eslint": "echo --JS source code linting-- && eslint src --ext .js && echo No JS problems found",
    "lint": "echo --Lint all sources-- && npm-run-all eslint",
    "jest-clear": "echo --Clear jest cache-- && jest --clearCache",
    "jest": "echo --Run tests-- && jest --verbose",
    "test": "echo --Run all lintings and tests-- && npm-run-all lint jest",
    "build-dev": "echo --Build development-- && webpack --mode development",
    "build-prod": "echo --Build production-- && webpack --mode production",
    "build": "echo --Build default-- && npm run -s build-prod",
    "dist": "echo --Create compiled lib distrubution package-- && babel -s -d dist src --verbose",
    "dist-watch": "echo --Watch compiled lib distrubution package-- && babel -d dist src --verbose --watch",
    "webpack-stats": "echo --Generate webpack stats-- && webpack --profile --json > .webpack-stats.json",
    "webpack-stats-analyze": "echo --Analyze webpack stats-- && npm run -s webpack-stats && webpack-bundle-analyzer .webpack-stats.json build",
    "webpack-example-build": "echo --Build webpack example-- && cd webpack-example && webpack --mode development",
    "webpack-example-build-prod": "echo --Build webpack example-- && cd webpack-example && webpack --mode production",
    "webpack-example-server": "echo --Webpack example demo server-- && cd webpack-example && webpack-dev-server --env.THEME=localServer --mode development --hot",
    "tags": "ctags -R --exclude=@.ctagsignore ."
  }
}
